{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=brewery.db"
  },
  "JwtSettings": {
    "SecretKey": "YourSuperSecretKeyForBreweryApiMinimum32Characters!",
    "Issuer": "BreweryApi",
    "Audience": "BreweryApiUsers",
    "ExpirationHours": 1
  },
  "CacheSettings": {
    "CacheDurationMinutes": 10
  }
}